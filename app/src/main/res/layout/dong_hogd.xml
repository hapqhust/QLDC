<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1050px"
    android:layout_height="280px"
    android:layout_marginEnd="5dp"
    android:layout_marginTop="8dp"
    android:layout_marginStart="5dp"
    android:layout_marginBottom="20dp">
<androidx.cardview.widget.CardView
    android:layout_width="1050px"
    android:layout_height="280px"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginBottom="5dp"

    app:cardBackgroundColor="#fefefe"
    app:cardCornerRadius="5dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageViewHouse"
            android:layout_width="71dp"
            android:layout_height="83dp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/house"
            tools:ignore="VectorDrawableCompat" />

        <TextView
            android:id="@+id/textViewIDHoGD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="16dp"
            android:layout_toRightOf="@id/imageViewHouse"
            android:text="ID Gia Dinh"
            android:textColor="#7C7C7C"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textViewIDChuHo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="20dp"
            android:text="ID Chu Ho"
            android:textColor="#7C7C7C"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textViewDiaChi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="16dp"
            android:layout_toRightOf="@id/imageViewHouse"
            android:layout_below="@id/textViewIDHoGD"
            android:text="Dia chi"
            android:textColor="#232323"
            android:textSize="19sp" />

    </RelativeLayout>
</androidx.cardview.widget.CardView>
</LinearLayout>